intY = intY + int(Rnd * 30 - 15)


dm.MoveTo intX, intY//点击加上随机数之后的坐标
	Delay int(Rnd * 10 + 3)*yanchi
   
    dm.leftdown
    Delay int(Rnd * 10 + 3)*yanchi
   
    dm.leftup
    TracePrint "点击"
    Delay int(Rnd * 10 + 10) * yanchi
    Delay int(Rnd * 20 + 40)*yanchi
   
    dm.leftdown
    Delay int(Rnd * 10 + 3)*yanchi
    
    dm.leftup
    TracePrint "点击"
    Delay int(Rnd * 10 + 3) * yanchi
    
    Call 随机点击
	Delay 500
	Call 点击战利品1
    

End If
End Sub

