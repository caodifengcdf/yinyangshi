intY = intY + int(Rnd * 10 + 6)


dm.MoveTo intX, intY//点击加上随机数之后的坐标
	Delay int(Rnd * 10 + 3)*yanchi
   
	call m_doubleclick
	
    
    Delay 2000
    call 退出探索确认

End If
End Sub

