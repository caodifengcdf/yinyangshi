Sub 随机点击
	//随机点击次数
Dim A
A = int(Rnd * 10 + 1)
TracePrint A
If A > 5 Then 
TracePrint"增加一次点击"
 
GetCursorPos x, y
intX = x
intY = y
dm.MoveTo intX, intY
	Delay int(Rnd * 10 + 3)*yanchi
    dm.leftdown
    Delay int(Rnd * 10 + 3)*yanchi
    dm.leftup
    TracePrint "点击"
    Delay int(Rnd * 10 + 3) * yanchi  
	
End If

End Sub
Sub 排除异常
//不小心点到人物
dm.FindPic fanwei_x,fanwei_y,fanwei_x+960,fanwei_y+570, pic_path&"人物误触.bmp", "000000", 0.8, 0, intX, intY
If intX > 0 And intY > 0 Then 
TracePrint "出现误触异常"
Delay int(Rnd * 10 + 3)*yanchi
intX = intX - int(Rnd * 90 + 30)
intY = intY - int(Rnd * 20 + 10)
dm.MoveTo intX, intY//点击加上随机数之后的坐标
	Delay int(Rnd * 10 + 3)*yanchi
   
	Call m_doubleclick
	
	Delay int(Rnd * 2000 + 4000)

End If

//不小心点到妖气
dm.FindPic fanwei_x,fanwei_y,fanwei_x+960,fanwei_y+570, pic_path&"妖气封印.bmp", "000000", 0.8, 0, intX, intY
If intX > 0 And intY > 0 Then 
TracePrint "出现误触异常"
Delay int(Rnd * 10 + 3)*yanchi
intX = intX - int(Rnd * 90 + 30)
intY = intY - int(Rnd * 20 + 10)
dm.MoveTo intX, intY//点击加上随机数之后的坐标
	Delay int(Rnd * 10 + 3)*yanchi
   
	Call m_doubleclick
	
	Delay int(Rnd * 2000 + 4000)

End If
//不小心点到式神
dm.FindPic fanwei_x,fanwei_y,fanwei_x+960,fanwei_y+570, pic_path&"关闭.bmp", "000000", 0.8, 0, intX, intY
If intX > 0 And intY > 0 Then 
TracePrint "出现误触异常"
Delay int(Rnd * 10 + 3)*yanchi
intX = intX - int(Rnd * 90 + 30)
intY = intY - int(Rnd * 20 + 10)
dm.MoveTo intX, intY//点击加上随机数之后的坐标
	Delay int(Rnd * 10 + 3)*yanchi
   
	Call m_doubleclick
	
	Delay int(Rnd * 2000 + 4000)

End If



End Sub
TracePrint"start"


Call 点击挑战
Call 点击接受邀请


While 1
//如果没有卡住

Call 点击挑战

Call 结束战斗重新开始
Call 结束战斗重新开始

Call 继续邀请
Call 开始邀请队友
Call 点击挑战
Call 排除异常

call 退出探索确认
Call 退出探索确认2
	While fuben1 = 1 or fuben2 = 1 //在副本中时
		//找到妖怪
 
		TracePrint "战斗吧"
		TracePrint fuben1&"和"&fuben2
		
		Call 点击妖怪
	
		Call 点击胜利
		Call 点击胜利1
		
		Call 点击战利品1
		Call 点击战利品
		Call 排除异常
		Call 界面判断
	Wend 
TracePrint"退出循环"	
Wend


